public static void main(String[] args) {

        System.out.println("number\t  square   cube");

        int i;
        for ( i=0; i < 11; i++) {
            System.out.print("   " + i);
            int square = i * i;
            System.out.print("\t" + "\t" + square);
            int cube = i * i * i;
            System.out.println("\t" + "\t" + cube);
        }
    }
